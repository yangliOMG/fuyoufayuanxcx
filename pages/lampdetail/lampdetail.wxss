.state-bar{display: flex;padding: 0 5px;}
.state-bar .lie{flex: 1 1;display: flex;}
.state-bar .lie .name{padding-top: 3px;line-height: 18px;text-align: center;}

.area{position: absolute;left: 0;right: 0;top: 45px;bottom: 129px;overflow: auto;}
.swiper{height: 100%;}
.swiper-item{overflow: auto;}
.lampTab{height:100%;padding:10px 5px;font-size:22px;}
.lampTab .rowNum{display:table;height:100%;width: 8%;margin-right: 4%;float: left;background: #fff;color: #333;box-shadow: 2px 0px 0px #aaa;border-radius: 36px;}
.lampTab .lampPannel{display:table;height:100%;width: 87%;float: left;line-height:1.5;text-align:center;position:relative;background: #fff;
    box-shadow: 2px 2px 2px #aaa;border-radius: 9px;margin-bottom: 5px;}

.fixed-bar{position: fixed;width: 100%;bottom: 0;}
.fixed-bar .field-bar{display: flex;line-height: 30px;align-items: stretch;padding: 5px;text-align: center;}
.titleCard{border-radius: 25px;background:white;overflow: hidden;}
.titleCard .channel-box{display: flex;transition: transform 0.3s cubic-bezier(0.35, 0, 0.25, 1);}
.titleCard .channel-box .channel{width: 100%;flex-shrink: 0;}
.leftArrow{width:32px;position: absolute;left: 85px;z-index:1;transition: all 0.5s ease;}
.leftArrow.left{transform: rotate3d(1, 0, 0, 360deg);left: 75px;}
.rightArrow{width:32px;position: absolute;right: 85px;z-index:1;transition: all 0.5s ease;}
.rightArrow.right{transform: rotate3d(1, 0, 0, 360deg);right: 75px;}
.fixed-bar .recom-bar{line-height: 30px;padding: 0 10px;height: 43px;text-align: left;display: flex;align-items: center;}
.fixed-bar .pos-bar{line-height: 30px;padding: 0 10px;height: 43px;text-align: left;}
.fixed-bar .pos-bar .nowrap{white-space: nowrap;overflow: auto;}
.fixed-bar .pos-bar .nowrap .nameplate{display: inline-block;padding: 0px 8px;margin: 8px 5px 5px 0;font-size: 12px;color: white;position: relative;}
.fixed-bar .pos-bar .nowrap .timecircle{color:white;position:absolute;right:-4px;top:-4px;font-size:14px;background:grey;width:20px;height:20px;border-radius:50%;line-height:17px;text-align:center;font-weight:bold;}
.fixed-bar .radiobutton{margin:0 7px 2px 0;display: inline-block;height: 30px;padding: 0px 10px;border-radius: 8px;color:orange;border: 1px solid orange;}
.fixed-bar .radiobutton:active{color:white;background: orange}
.fixed-bar .seled-bar{line-height: 30px;text-align: left;color: white;}






.lampIcon{line-height: 0;margin: 0 auto;display: flex;vertical-align: middle;border: 0 none;outline: none;background-color: transparent;background-repeat: no-repeat;background-attachment: scroll;
    width: 32px;height: 40px;background-size:520%;background-image: url('data:image/png;base64,');}
.lampIcon.l-gong,.lampIcon.l-gong-mini{background-position: 0 0;animation-duration: 3s;animation-timing-function: steps(2, end);animation-iteration-count: infinite;}
.lampIcon.l-gong{animation-name:mymove;}
.lampIcon.l-gong-mini{animation-name:mymove1;}
@keyframes mymove {100% {background-position: -196px 0;}}
@keyframes mymove1 {100% {background-position: -167px 0;}}

.lampIcon.l-shan{background-position: 0 0;}
.lampIcon.l-bushan{background-position: -82px 0;}
.lampIcon.tini{width: 19px;height:23px;display: inline-block;vertical-align: bottom;}

.lampIcon.l-red{background-position: -49px 0;}
.lampIcon.l-red.mini{background-position-x:-42px;}
.lampIcon.l-grey{background-position: -139px 0;}
.lampIcon.l-grey.mini{background-position-x:-118px;}
.lampIcon.mini{width: 27px;height:33px;}